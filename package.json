{
  "name": "crpr",
  "version": "0.2.0",
  "packageManager": "pnpm@8.15.6",
  "author": "Ray <i@mk1.io> (@so1ve)",
  "description": "Create a promise, but avoid using constructors.",
  "keywords": [
    "prromise",
    "utils"
  ],
  "homepage": "https://github.com/so1ve/crpr#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/crpr.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/crpr/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "tsx src/index.ts",
    "stub": "unbuild --stub",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cride": "^0.4.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.0",
    "@babel/types": "^7.21.5",
    "@so1ve/eslint-config": "^0.110.0",
    "@types/node": "20.12.5",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.8",
    "vitest": "^0.32.0"
  }
}
